<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="net.a1337ism.database.MessageLoggerDbModel" table="r_crossdressing">
      <id name="id" type="int" column="id">
         <generator class="sequence">
         	<param name="sequence">r_crossdressing_id_seq</param>
         </generator>
      </id>
      <property name="nickname" column="nickname" type="string"/>
      <property name="username" column="username" type="string"/>
      <property name="hostmask" column="hostmask" type="string"/>
      <property name="message" column="message" type="string"/>
      <property name="channel" column="channel" type="string"/>
      <property name="time" column="time" type="timestamp"/>
      <property name="type" column="type" type="string"/>
   </class>
</hibernate-mapping>